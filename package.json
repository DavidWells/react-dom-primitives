{
  "name": "react-dom-primitives",
  "version": "0.0.1",
  "description": "Base set of React DOM Components",
  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "clean-lib": "node_modules/.bin/rimraf ./lib",
    "build": "npm run clean-lib && npm run build:functional && npm run build:utils && npm run build:class && npm run build:index && npm run copypackage",
    "build:class": "webpack --config webpack.production.class.config.js --colors --progress --inline",
    "build:functional": "webpack --config webpack.production.config.js --colors --progress --inline",
    "build:utils": "webpack --config webpack.production.utils.config.js --colors --progress --inline",
    "build:index": "babel src/primatives/index.js --out-file lib/index.js",
    "b": "babel src/primatives/A/A.functional.js --out-file lib/test.js",
    "copypackage": "cp -rf package.json lib && cp -rf readme.md lib",
    "dist": "cd lib && npm publish"
  },
  "author": "David Wells",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DavidWells/react-primatives.git"
  },
  "peerDependencies": {
    "react": "^0.14"
  },
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.26",
    "babel-eslint": "^5.0.0-beta4",
    "babel-loader": "^6.2.0",
    "babel-plugin-react-transform": "^2.0.0-beta1",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "bluebird": "^3.1.1",
    "copy-template-dir": "^1.2.1",
    "core-js": "^1.2.6",
    "cpx": "^1.2.1",
    "cross-env": "^1.0.5",
    "css-loader": "^0.23.1",
    "eslint": "^1.10.3",
    "eslint-plugin-babel": "^3.0.0",
    "eslint-plugin-react": "^3.12.0",
    "expect": "^1.13.4",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^0.9.1",
    "glob": "^6.0.2",
    "history": "^1.17.0",
    "mocha": "^2.3.4",
    "phantomjs": "^1.9.19",
    "phantomjs-polyfill": "0.0.1",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-hot-loader": "^1.2.7",
    "react-router": "^2.0.0",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.1",
    "redbox-react": "^1.2.2",
    "rimraf": "^2.5.0",
    "webpack": "^1.12.9",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.6.0"
  }
}
